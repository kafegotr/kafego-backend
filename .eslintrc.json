{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
       "no-console": "off",
       "eslint-disable-next-line": "off",
       "import/prefer-default-export": "off",
       "no-unused-vars": "off",
       "no-undef": "off",
       "no-return-await": "off",
       "camelcase": "off",
       "import/extension": "off"
    }
}